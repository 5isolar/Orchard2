{
  "version": "2.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Modules": "1.0.0-*",
    "NETStandard.Library": "1.6.1",
    "Orchard.Admin.Abstractions": "2.0.0-*",
    "Orchard.ContentManagement": "2.0.0-*",
    "Orchard.ContentManagement.Abstractions": "2.0.0-*",
    "Orchard.ContentManagement.Display": "2.0.0-*",
    "Orchard.Environment.Navigation": "2.0.0-*",
    "Orchard.Recipes.Abstractions": "2.0.0-*",
    "Orchard.Scripting.Abstractions": "2.0.0-*",
    "Orchard.SiteSettings.Core": "2.0.0-*",
